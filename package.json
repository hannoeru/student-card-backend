    "@types/passport-twitter": "^1.0.36",
    "@types/uuid": "^8.3.0",
    "eslint": "^7.16.0",
    "esno": "^0.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.3.0",
    "@prisma/client": "^2.13.1",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.9.8",
