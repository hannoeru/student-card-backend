  },
  "homepage": "https://github.com/hannoeru/student-card-backend#readme",
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.4.3",
    "@prisma/cli": "^2.13.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-serve-static-core": "^4.17.17",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
